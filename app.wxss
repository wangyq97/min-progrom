/**app.wxss**/
/* 引入字体图标 */
@import "./style/iconfont.wxss";

/* reset 所有的 padding margin */
page,view,text,swiper,swiper-item,image,navigator {
    padding:0;
    margin: 0;
    box-sizing: border-box;
}

/* 主体颜色 */
page {
    /* 定义主体颜色 */
    --themeColor:#eb4450;
    /* 定义字体大小 */
    font-size: 28rpx;
}
