/**index.wxss**/
.container .index_swiper {
  width: 750rpx;
  height: 340rpx;
}
.container .index_swiper image {
  width: 100%;
}
.container .index_cate {
  display: flex;
}
.container .index_cate navigator {
  padding: 20rpx;
  flex: 1;
}
.container .index_cate navigator image {
  width: 100%;
}
.container .index_floor .groop .title {
  padding: 10rpx 0;
}
.container .index_floor .groop .title image {
  width: 100%;
}
.container .index_floor .groop .content {
  overflow: hidden;
}
.container .index_floor .groop .content .content_item {
  float: left;
  width: 33.33%;
}
.container .index_floor .groop .content .content_item:nth-last-child(-n+4) {
  height: 27.72711207vw;
  border-left: 10rpx solid #fff;
}
.container .index_floor .groop .content .content_item:nth-child(3),
.container .index_floor .groop .content .content_item:nth-child(2) {
  border-bottom: 10rpx solid #fff;
}
.container .index_floor .groop .content .content_item image {
  width: 100%;
  height: 100%;
}
